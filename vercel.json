{
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:js|css|tsx|ts|json|png|jpg|jpeg|svg|gif|ico|woff|woff2|ttf|otf)$).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}